parameters:
    level: 1
    reportUnmatchedIgnoredErrors: false
    ignoreErrors:
        - '#Variable \$this might not be defined.#'
        - '#Variable \$block might not be defined.#'
        - '#Undefined variable: \$this#'
        - '#Undefined variable: \$block#'
    fileExtensions:
        - php
        - phtml
    excludes_analyse:
        - Test/*
